            <Image url="@../accest/icons8-supermarket-64.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="36.0" fitWidth="35.0" layoutX="881.0" layoutY="22.0" onMouseClicked="#goLogOut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../accest/icons8-logout-rounded-left-96.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
