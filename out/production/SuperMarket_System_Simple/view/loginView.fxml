<AnchorPane prefHeight="542.0" prefWidth="838.0" style="-fx-background-color: #3d75ad;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginViewController">
   <children>
      <AnchorPane fx:id="loginContext" layoutY="86.0" prefHeight="487.0" prefWidth="935.0" style="-fx-background-color: #ffffff;">
         <children>
            <ImageView fitHeight="487.0" fitWidth="616.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../accest/undraw_shopping_app_flsj.png" />
               </image>
            </ImageView>
            <AnchorPane layoutX="603.0" layoutY="60.0" prefHeight="337.0" prefWidth="287.0" style="-fx-background-radius: 25; -fx-background-color: #e4e4e4;" />
            <TextField fx:id="txtUserName" layoutX="619.0" layoutY="184.0" prefHeight="31.0" prefWidth="254.0" />
            <PasswordField fx:id="pwdPassword" layoutX="619.0" layoutY="256.0" prefHeight="31.0" prefWidth="254.0" />
            <Button fx:id="btnLogin" layoutX="774.0" layoutY="313.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="38.0" prefWidth="95.0" style="-fx-background-radius: 25;" text="Log In">
