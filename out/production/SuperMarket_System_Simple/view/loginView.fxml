         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <ImageView fitHeight="70.0" fitWidth="63.0" layoutX="22.0" layoutY="10.0" onMouseClicked="#goToHome" pickOnBounds="true" preserveRatio="true">
